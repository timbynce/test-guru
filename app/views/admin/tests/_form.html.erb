<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>
  
  <div class="form-group">
    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title, id: :test_title, autofocus: true, autocomplete: "title", class: 'form-control', placeholder: "Test 1"  %>
    </p><br>

    <p>
      <%= form.label :level %><br>
      <%= form.text_field :level, id: :test_level,autofocus: true, autocomplete: "level", class: 'form-control' %>
    </p><br>

    <p>
      <%= form.label :category_id %><br>
      <%= form.collection_select :category_id, Category.all, :id, :title_translated, promt: true %>
    </p>

    <p><%= form.submit class: 'btn btn-primary mt-1' %></p>
  </div>
<% end %>
